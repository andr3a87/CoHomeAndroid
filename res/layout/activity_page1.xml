<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.cohome.android.SearchAd"
    tools:ignore="MergeRootFrame" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" 
        android:background="#edded6"
        >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/box1" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="152dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:text="Inserisci Località"
            android:textColor="#fff"
            android:textSize="18dp"
            android:textStyle="bold" />
		
		<AutoCompleteTextView
		    android:id="@+id/autoComplete"
		    android:layout_width="345dp"
            android:layout_height="35dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="40dp"
            android:layout_marginTop="50dp"
            android:background="#fff"
            android:ems="10"
		    android:text="" />
        
        <ImageView
            android:id="@+id/boxCalendarStart"
            android:layout_width="161dp"
            android:layout_height="161dp"
            android:src="@drawable/box_calendar" 
            android:layout_marginLeft="20dp"
            android:layout_marginTop="120dp"
            />
        
        <ImageView
            android:id="@+id/boxCalendarEnd"
            android:layout_width="161dp"
            android:layout_height="161dp"
            android:src="@drawable/box_calendar" 
            android:layout_marginLeft="200dp"
            android:layout_marginTop="120dp"
            />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="135dp"
            android:layout_marginLeft="40dp"
            android:text="ARRIVO" 
            android:textColor="#05bdf9"
            />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_calendar" 
            android:layout_marginTop="130dp"
            android:layout_marginLeft="135dp"
            />

        <TextView
            android:id="@+id/textDateStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_marginTop="250dp"
            android:layout_marginLeft="70dp"
            
             />
        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="135dp"
            android:layout_marginLeft="230dp"
            android:text="PARTENZA" 
            android:textColor="#05bdf9"
            />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_calendar" 
            android:layout_marginTop="130dp"
            android:layout_marginLeft="315dp"
            />

        <TextView
            android:id="@+id/textDateEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_marginTop="250dp"
            android:layout_marginLeft="250dp"
             />
		<TextView
            android:id="@+id/textDayStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="44dp"
            android:textStyle="bold"
            android:layout_marginTop="170dp"
            android:layout_marginLeft="40dp"
            android:text="" />
		<TextView
            android:id="@+id/textMonthStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:layout_marginTop="180dp"
            android:layout_marginLeft="100dp"
            android:text="" />
		<TextView
            android:id="@+id/textYearStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:layout_marginTop="200dp"
            android:layout_marginLeft="100dp"
            android:text="" />
		<TextView
            android:id="@+id/textDayOfWeekStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:layout_marginTop="220dp"
            android:layout_marginLeft="40dp"
            android:text="" />
		<TextView
            android:id="@+id/textDayEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="44dp"
            android:textStyle="bold"
            android:layout_marginTop="170dp"
            android:layout_marginLeft="220dp"
            android:text="" />
		<TextView
            android:id="@+id/textMonthEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:layout_marginTop="180dp"
            android:layout_marginLeft="280dp"
            android:text="" />
		<TextView
            android:id="@+id/textYearEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15dp"
            android:layout_marginTop="200dp"
            android:layout_marginLeft="280dp"
            android:text="" />
		<TextView
            android:id="@+id/textDayOfWeekEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12dp"
            android:layout_marginTop="220dp"
            android:layout_marginLeft="220dp"
            android:text="" />

		<ImageView
		    android:id="@+id/imageView2"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/box2" 
		    android:layout_marginTop="320dp"
            android:layout_marginLeft="15dp"
		    />

		<TextView
		    android:id="@+id/textView3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="N° OSPITI" 
		    android:layout_marginTop="338dp"
            android:layout_marginLeft="25dp"
            android:textColor="#05bdf9"
            android:textSize="25dp"
		    />

		<TextView
		    android:id="@+id/editTextOspiti"
		    android:layout_width="50dp"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="270dp"
		    android:layout_marginTop="345dp"
		    android:ems="10"
		    android:text="1"
		    android:inputType="number" 
		    android:textSize="18dp"
		    />

		<ImageView
		    android:id="@+id/imageAdd"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/add" 
		    android:layout_marginLeft="300dp"
		    android:layout_marginTop="345dp"
		    />

		<ImageView
		    android:id="@+id/imageLess"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/less" 
		    android:layout_marginLeft="230dp"
		    android:layout_marginTop="345dp"
		    />

		<ImageView
		    android:id="@+id/imageCerca"
		    android:layout_width="100dp"
		    android:layout_height="50dp"
		    android:src="@drawable/box3" 
		    android:layout_marginLeft="140dp"
		    android:layout_marginTop="420dp"
		    />

		<TextView
		    android:id="@+id/textView5"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="Cerca" 
		    android:layout_marginLeft="162dp"
		    android:layout_marginTop="430dp"
		    android:textColor="#fff"
            android:textSize="22dp"
            android:textStyle="bold"
		    />

    </FrameLayout>
</FrameLayout>