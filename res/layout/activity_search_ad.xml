<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="0dp"
    android:layout_marginTop="0dp"
    tools:context="com.cohome.android.SearchAd"
    tools:ignore="MergeRootFrame" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="0dp"
        android:background="#edded6" >

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/box1" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="152dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="20dp"
            android:text="Inserisci Località"
            android:textColor="#fff"
            android:textSize="18dp"
            android:textStyle="bold" />
		
		<AutoCompleteTextView
		    android:id="@+id/autoComplete"
		    android:layout_width="279dp"
		    android:layout_height="35dp"
		    android:layout_marginLeft="20dp"
		    android:layout_marginRight="40dp"
		    android:layout_marginTop="50dp"
		    android:background="#fff"
		    android:ems="10"
		    android:text="" />
        
        <ImageView
            android:id="@+id/boxCalendarStart"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:src="@drawable/box_calendar" 
            android:layout_marginLeft="10dp"
            android:layout_marginTop="100dp"
            />
        
        <ImageView
            android:id="@+id/boxCalendarEnd"
            android:layout_width="140dp"
            android:layout_height="140dp"
            android:src="@drawable/box_calendar" 
            android:layout_marginLeft="170dp"
            android:layout_marginTop="100dp"
            />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="110dp"
            android:layout_marginLeft="30dp"
            android:text="ARRIVO" 
            android:textColor="#05bdf9"
            />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_calendar" 
            android:layout_marginTop="105dp"
            android:layout_marginLeft="115dp"
            />

        <TextView
            android:id="@+id/textDateStart"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="45dp"
            android:layout_marginTop="220dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="110dp"
            android:layout_marginLeft="190dp"
            android:text="PARTENZA" 
            android:textColor="#05bdf9"
            />

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:src="@drawable/ic_calendar" 
            android:layout_marginTop="105dp"
            android:layout_marginLeft="275dp"
            />

        <TextView
            android:id="@+id/textDateEnd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="220dp" />

		<TextView
		    android:id="@+id/textDayStart"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="30dp"
		    android:layout_marginTop="150dp"
		    android:textSize="44dp"
		    android:textStyle="bold" />

		<TextView
		    android:id="@+id/textMonthStart"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="100dp"
		    android:layout_marginTop="155dp"
		    android:textSize="15dp" />

		<TextView
		    android:id="@+id/textYearStart"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="100dp"
		    android:layout_marginTop="175dp"
		    android:textSize="15dp" />

		<TextView
		    android:id="@+id/textDayOfWeekStart"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="35dp"
		    android:layout_marginTop="197dp"
		    android:textSize="12dp" />

		<TextView
		    android:id="@+id/textDayEnd"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="190dp"
		    android:layout_marginTop="150dp"
		    android:textSize="44dp"
		    android:textStyle="bold" />

		<TextView
		    android:id="@+id/textMonthEnd"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="260dp"
		    android:layout_marginTop="155dp"
		    android:textSize="15dp" />

		<TextView
		    android:id="@+id/textYearEnd"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="260dp"
		    android:layout_marginTop="175dp"
		    android:textSize="15dp" />

		<TextView
		    android:id="@+id/textDayOfWeekEnd"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="195dp"
		    android:layout_marginTop="197dp"
		    android:textSize="12dp" />

		<ImageView
		    android:id="@+id/imageView2"
		    android:layout_width="296dp"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="10dp"
		    android:layout_marginTop="300dp"
		    android:src="@drawable/box2" />

		<TextView
		    android:id="@+id/textView3"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="N° OSPITI" 
		    android:layout_marginTop="320dp"
            android:layout_marginLeft="20dp"
            android:textColor="#05bdf9"
            android:textSize="25dp"
		    />

		<TextView
		    android:id="@+id/editTextOspiti"
		    android:layout_width="50dp"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="240dp"
		    android:layout_marginTop="325dp"
		    android:ems="10"
		    android:text="1"
		    android:inputType="number" 
		    android:textSize="18dp"
		    />

		<ImageView
		    android:id="@+id/imageAdd"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/add" 
		    android:layout_marginLeft="270dp"
		    android:layout_marginTop="325dp"
		    />

		<ImageView
		    android:id="@+id/imageLess"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:src="@drawable/less" 
		    android:layout_marginLeft="200dp"
		    android:layout_marginTop="325dp"
		    />

		<ImageView
		    android:id="@+id/imageCerca"
		    android:layout_width="100dp"
		    android:layout_height="50dp"
		    android:src="@drawable/box3" 
		    android:layout_marginLeft="110dp"
		    android:layout_marginTop="400dp"
		    />

		<TextView
		    android:id="@+id/textView5"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:text="Cerca" 
		    android:layout_marginLeft="132dp"
		    android:layout_marginTop="410dp"
		    android:textColor="#fff"
            android:textSize="22dp"
            android:textStyle="bold"
		    />

    </FrameLayout>
</FrameLayout>